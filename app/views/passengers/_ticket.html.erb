<div class="ticket-container mb-3">
  <ul class="left">
    <li></li>
    <li></li>
    <li></li>
    <li></li>
    <li></li>
    <li></li>
    <li></li>
    <li></li>
    <li></li>
    <li></li>
    <li></li>
    <li></li>
    <li></li>
    <li></li>
  </ul>
  
  <ul class="right">
    <li></li>
    <li></li>
    <li></li>
    <li></li>
    <li></li>
    <li></li>
    <li></li>
    <li></li>
    <li></li>
    <li></li>
    <li></li>
    <li></li>
    <li></li>
    <li></li>
  </ul>
  <div class="ticket">
    <span class="airline">Odin Airlines</span>
    <span class="airline airlineslip">Odin Airlines</span>
    <span class="boarding">BOARDING PASS</span>
    <div class="content">
      <span class="origin"><%= @flight.origin.code %></span>
      <span class="origincity"><%= @flight.origin.city %></span>
      <span class="plane"><?xml version="1.0" ?><svg clip-rule="evenodd" fill-rule="evenodd" height="60" width="60" image-rendering="optimizeQuality" shape-rendering="geometricPrecision" text-rendering="geometricPrecision" viewBox="0 0 500 500" xmlns="http://www.w3.org/2000/svg"><g stroke="#222"><line fill="none" stroke-linecap="round" stroke-width="30" x1="300" x2="55" y1="390" y2="390"/><path d="M98 325c-9 10 10 16 25 6l311-156c24-17 35-25 42-50 2-15-46-11-78-7-15 1-34 10-42 16l-56 35 1-1-169-31c-14-3-24-5-37-1-10 5-18 10-27 18l122 72c4 3 5 7 1 9l-44 27-75-15c-10-2-18-4-28 0-8 4-14 9-20 15l74 63z" fill="#222" stroke-linejoin="round" stroke-width="10"/></g></svg></span>
      <span class="dest"><%= @flight.destination.code %></span>
      <span class="destcity"><%= @flight.destination.city %></span>
      
      <span class="origin originslip"><%= @flight.origin.code %></span>
      <span class="origincity origincityslip"><%= @flight.origin.city %></span>
      <span class="plane planeslip"><?xml version="1.0" ?><svg clip-rule="evenodd" fill-rule="evenodd" height="50" width="50" image-rendering="optimizeQuality" shape-rendering="geometricPrecision" text-rendering="geometricPrecision" viewBox="0 0 500 500" xmlns="http://www.w3.org/2000/svg"><g stroke="#222"><line fill="none" stroke-linecap="round" stroke-width="30" x1="300" x2="55" y1="390" y2="390"/><path d="M98 325c-9 10 10 16 25 6l311-156c24-17 35-25 42-50 2-15-46-11-78-7-15 1-34 10-42 16l-56 35 1-1-169-31c-14-3-24-5-37-1-10 5-18 10-27 18l122 72c4 3 5 7 1 9l-44 27-75-15c-10-2-18-4-28 0-8 4-14 9-20 15l74 63z" fill="#222" stroke-linejoin="round" stroke-width="10"/></g></svg></span>
      <span class="dest destslip"><%= @flight.destination.code %></span>
      <span class="destcity destcityslip"><%= @flight.destination.city %></span>
      <div class="sub-content">
        <span class="watermark">ODIN AIRLINES</span>
        <span class="name">PASSENGER NAME<br><span class="text-uppercase"><%= @passenger.last_name %>, <%= @passenger.first_name %></span></span>
        <span class="flight">FLIGHT<br><span><%= @flight.flight_num %></span></span>
        <span class="gate">GATE<br><span><%= @gate %></span></span>
        <span class="seat">SEAT<br><span><%= @seat %></span></span>
        <span class="boardingtime">BOARDING TIME<br><span><%= @flight.departure_date.strftime("%d %b %Y") %> <%= @flight.departure_time %></span></span>
            
         <span class="flight flightslip">FLIGHT<br><span><%= @flight.flight_num %></span></span>
          <span class="seat seatslip">SEAT<br><span><%= @seat %></span></span>
         <span class="name nameslip">PASSENGER NAME<br><span class="text-uppercase"><%= @passenger.last_name %>, <%= @passenger.first_name %></span></span>
      </div>
    </div>
    <div class="barcode"></div>
    <div class="barcode slip"></div>
  </div>
</div>